<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.simonme.demo.hibernate.model">

    <class name="TestBean" table="test">
        <id name="id" column="id">
            <generator class="native"></generator>
        </id>
        <property name="name" column="name" />
        <property name="desciption" column="description" />
        <property name="updateDate" type="timestamp" column="update_date"/>
        <property name="testLong" column="test_long" type="java.lang.Long" />
    </class>

</hibernate-mapping>
